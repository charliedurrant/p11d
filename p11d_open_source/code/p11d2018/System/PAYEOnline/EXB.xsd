        <xsd:attribute use="required" name="Category">
          <xsd:simpleType>
            <xsd:restriction base="xsd:integer">
              <xsd:enumeration value="4">
                <xsd:annotation>
                  <xsd:documentation>0 - No engine size e.g. electric car</xsd:documentation>
                </xsd:annotation>
              </xsd:enumeration>
              <xsd:enumeration value="3">
                <xsd:annotation>
                  <xsd:documentation>2001cc or more</xsd:documentation>
                </xsd:annotation>
              </xsd:enumeration>
              <xsd:enumeration value="2">
                <xsd:annotation>
                  <xsd:documentation>1401 - 2000cc</xsd:documentation>
                </xsd:annotation>
              </xsd:enumeration>
              <xsd:enumeration value="1">
                <xsd:annotation>
                  <xsd:documentation>Up to 1400cc</xsd:documentation>
                </xsd:annotation>
              </xsd:enumeration>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>

Private Sub IBenefitForm_AddBenefit()
  Dim benAccom As clsBenAccom
  Dim ben As IBenefitClass
  Dim lst As ListItem
  Dim i As Long
  On Error GoTo AddBenefit_Err
  Call xSet("AddBenefit")
  
  Set benAccom = New clsBenAccom
  Set ben = benAccom
  
  'Put in defaults for benefit ( zzzz Use defaults from the database?? - Good idea
  Set ben.Parent = CurrentEmployee
  Call ben.SetItem(accom_employeereference, CurrentEmployee.PersonelNo)
  
  Call ben.SetItem(accom_item, "Please enter address")
  Call ben.SetItem(accom_rent, 0)
  Call ben.SetItem(accom_business, 0)
  Call ben.SetItem(accom_Unavailable, 0)
  Call ben.SetItem(accom_RelevantDays, 0)
  Call ben.SetItem(accom_consideration, 0)
  Call ben.SetItem(accom_Price, 0)
  Call ben.SetItem(accom_Improv, 0)
  Call ben.SetItem(accom_CapContrib, 0)
  Call ben.SetItem(accom_Tenancy, 0)
  Call ben.SetItem(accom_MVFirstOcc, 0)
  Call ben.SetItem(accom_RecentImprov, 0)
  Call ben.SetItem(accom_Utilities, 0)
  Call ben.SetItem(accom_Repairs, 0)
  Call ben.SetItem(accom_Assets, 0)
  Call ben.SetItem(accom_Reimbursements, 0)
  Call ben.SetItem(accom_MadeGood, 0)
  Call ben.SetItem(accom_NetEmoluments, 0)
  Call ben.SetItem(accom_AvailDays, 0)
  Call ben.SetItem(accom_CoP, 0)
  Call ben.SetItem(accom_Ancillary, 0)
  Call ben.SetItem(accom_nemployees, 0)
    
  Call ben.SetItem(accom_Availablefrom, rates.GetItem(taxyearstart))
  Call ben.SetItem(accom_Availableto, rates.GetItem(taxyearend))
  Call ben.SetItem(accom_FirstOcc, rates.GetItem(accomFirstOccupiedDef))

  
  Call MDIMain.SetConfirmUndo
  ben.Dirty = True
  ben.ReadFromDB = True
  i = CurrentEmployee.benefits.Add(ben)
  
  Set lst = lb.ListItems.Add(, , ben.name)
  With lst
    .Tag = i
    .Text = ben.name
    .SubItems(1) = formatworkingnumber(ben.Calculate, "")
  End With
  
  Call AccomDetails(lst.Tag)
  Set lb.SelectedItem = lst
  fmeAll.Enabled = True
  lb.Enabled = True
  cmdTab(1).Enabled = False
  Call MDIMain.SetDelete
AddBenefit_End:
  Set ben = Nothing
  Call xReturn("AddBenefit")
  Exit Sub
AddBenefit_Err:
  Call ErrorMessage(ERR_ERROR, Err, "AddBenefit", "ERR_ADDBENEFIT", "Error in AddBenefit function, called from the form " & Me.name & ".")
  Resume AddBenefit_End
End Sub


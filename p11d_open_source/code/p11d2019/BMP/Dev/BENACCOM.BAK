Private Function IBenefitForm_Remove(i As Long) As Boolean
  On Error GoTo KillBenefit_Err
  Call xSet("KillBenefit")
  If Not benefit.CompanyDefined Then
    Call benefit.DeleteDB
    Call CurrentEmployee.benefits.Remove(i)
    Call IBenefitForm_ClearFields
    Call IBenefitForm_ListBenefits
  End If
KillBenefit_End:
  Call xReturn("KillBenefit")
  Exit Function
KillBenefit_Err:
  Call ErrorMessage(ERR_ERROR, Err, "KillBenefit", "ERR_UNDEFINED", "Undefined error.")
  Resume KillBenefit_End
End Function

Private Function IBenefitForm_ListBenefits(Optional ByVal Index As Long = 0&) As Boolean
  Dim ben As IBenefitClass
  Dim benfrm As IBenefitForm
  Dim lst As ListItem
  Dim i As Long, j As Long
  Dim lben As Variant
  
  On Error GoTo F_BenAccom_ListBenefits_Err
  Call xSet("F_BenAccom_ListBenefits")
  i = 0
  Call LockWindowUpdate(lb.hWnd)
  Set benfrm = Me
  Me.lb.ListItems.Clear
  benfrm.ClearFields
  Call MDIMain.SetAdd
  For i = 1 To CurrentEmployee.benefits.count
    Set ben = CurrentEmployee.benefits(i)
    If Not (ben Is Nothing) Then
      If ben.BenefitClass = BC_ACCOM Then
        lben = ben.Calculate
        Set lst = lb.ListItems.Add(, , ben.name)
        lst.Tag = i
        If VarType(lben) = vbString Then
          lst.SubItems(1) = lben
        Else
          lst.SubItems(1) = formatworkingnumber(lben, "")
        End If
        j = 1
      End If
    End If
  Next i
  If j = 0 Then
    lb.Enabled = False
    fmeAll.Enabled = False
    Call MDIMain.ClearDelete
    Call MDIMain.ClearConfirmUndo
    Set Me.benefit = Nothing
  Else
    Me.fmeAll.Enabled = True
    Me.lb.Enabled = True
    Call MDIMain.SetDelete
    lb.SelectedItem = lb.ListItems(1)
    Call AccomDetails(lb.SelectedItem.Tag)
  End If
F_BenAccom_ListBenefits_End:
  Call LockWindowUpdate(0)
  Call xReturn("F_BenAccom_ListBenefits")
  Exit Function
  
F_BenAccom_ListBenefits_Err:
  Call ErrorMessage(ERR_ERROR, Err, "F_BenAccom_ListBenefits", "Accomodation Benefits", "Error listing benefits.")
  Resume F_BenAccom_ListBenefits_End
End Function

Private Sub cmdTab_Click(Index As Integer)
  Dim i As Long
  For i = 0 To 2
    If i = Index Then
      cmdTab(i).Enabled = False
      fmeInput(i).Visible = True
    Else
      cmdTab(i).Enabled = True
      fmeInput(i).Visible = False
    End If
  Next i
  If Not benefit Is Nothing Then
    If cmdTab(1).Enabled = True Then cmdTab(1).Enabled = benefit.GetItem(accom_ErOwn)
  End If
End Sub

Private Function CheckChanged(ctl As Control) As Boolean
  Dim lst As ListItem
  Dim i As Long, d0 As Variant
  On Error GoTo CheckChanged_Err
  Call xSet("CheckChanged")
  
  If CurrentEmployee Is Nothing Then GoTo CheckChanged_End
  If benefit Is Nothing Then GoTo CheckChanged_End
  Select Case ctl.name
    Case "TxtBx"
      Select Case ctl.Index
        Case 1
          d0 = TryConvertDateDMY(ctl.Text)
          i = d0 <> benefit.GetItem(accom_Availablefrom)
          If i <> 0 Then Call benefit.SetItem(accom_Availablefrom, d0)
        Case 2
          i = StrComp(ctl.Text, benefit.GetItem(accom_item), vbBinaryCompare)
          If i <> 0 Then Call benefit.SetItem(accom_item, ctl.Text)
        Case 4
          d0 = TryConvertDateDMY(ctl.Text)
          i = d0 <> benefit.GetItem(accom_Availableto)
          If i <> 0 Then Call benefit.SetItem(accom_Availableto, d0)
        Case 8
          i = StrComp(ctl.Text, benefit.GetItem(accom_nemployees), vbBinaryCompare)
          If i <> 0 Then Call benefit.SetItem(accom_nemployees, ctl.Text)
        Case 15
          i = StrComp(ctl.Text, benefit.GetItem(accom_rent), vbBinaryCompare)
          If i <> 0 Then Call benefit.SetItem(accom_rent, ctl.Text)
        Case 19
          i = StrComp(ctl.Text, benefit.GetItem(accom_business), vbBinaryCompare)
          If i <> 0 Then Call benefit.SetItem(accom_business, ctl.Text)
        Case 20
          i = StrComp(ctl.Text, benefit.GetItem(accom_consideration), vbBinaryCompare)
          If i <> 0 Then Call benefit.SetItem(accom_consideration, ctl.Text)
        Case 3
          i = StrComp(ctl.Text, benefit.GetItem(accom_Price), vbBinaryCompare)
          If i <> 0 Then Call benefit.SetItem(accom_Price, ctl.Text)
        Case 9
          i = StrComp(ctl.Text, benefit.GetItem(accom_Improv), vbBinaryCompare)
          If i <> 0 Then Call benefit.SetItem(accom_Improv, ctl.Text)
        Case 11
          i = StrComp(ctl.Text, benefit.GetItem(accom_CapContrib), vbBinaryCompare)
          If i <> 0 Then Call benefit.SetItem(accom_CapContrib, ctl.Text)
        Case 14
          i = StrComp(ctl.Text, benefit.GetItem(accom_Tenancy), vbBinaryCompare)
          If i <> 0 Then Call benefit.SetItem(accom_Tenancy, ctl.Text)
        Case 18
          d0 = TryConvertDateDMY(ctl.Text)
          i = d0 <> benefit.GetItem(accom_FirstOcc)
          If i <> 0 Then
            Call benefit.SetItem(accom_FirstOcc, d0)
            Lab(28).Caption = "Improvements between " & benefit.GetItem(accom_FirstOcc) & " and " & rates.GetItem(taxyearstart)
          End If
        Case 13
          i = StrComp(ctl.Text, benefit.GetItem(accom_MVFirstOcc), vbBinaryCompare)
          If i <> 0 Then Call benefit.SetItem(accom_MVFirstOcc, ctl.Text)
        Case 17
          i = StrComp(ctl.Text, benefit.GetItem(accom_RecentImprov), vbBinaryCompare)
          If i <> 0 Then Call benefit.SetItem(accom_RecentImprov, ctl.Text)
        Case 0
          i = StrComp(ctl.Text, benefit.GetItem(accom_Utilities), vbBinaryCompare)
          If i <> 0 Then Call benefit.SetItem(accom_Utilities, ctl.Text)
        Case 5
          i = StrComp(ctl.Text, benefit.GetItem(accom_Repairs), vbBinaryCompare)
          If i <> 0 Then Call benefit.SetItem(accom_Repairs, ctl.Text)
        Case 6
          i = StrComp(ctl.Text, benefit.GetItem(accom_Reimbursements), vbBinaryCompare)
          If i <> 0 Then Call benefit.SetItem(accom_Reimbursements, ctl.Text)
        Case 10
          i = StrComp(ctl.Text, benefit.GetItem(accom_MadeGood), vbBinaryCompare)
          If i <> 0 Then Call benefit.SetItem(accom_MadeGood, ctl.Text)
        Case 12
          i = StrComp(ctl.Text, benefit.GetItem(accom_Ancillary), vbBinaryCompare)
          If i <> 0 Then Call benefit.SetItem(accom_Ancillary, ctl.Text)
        Case 16
          i = StrComp(ctl.Text, benefit.GetItem(accom_NetEmoluments), vbBinaryCompare)
          If i <> 0 Then Call benefit.SetItem(accom_NetEmoluments, ctl.Text)
        Case Else
          ECASE "Unknown control"
      End Select
    Case "ChkBx"
      Select Case ctl.Index
        Case 0
          i = (IIf(ctl.value = vbChecked, True, False) <> benefit.GetItem(accom_ErOwn))
          If i <> 0 Then
            Call benefit.SetItem(accom_ErOwn, IIf(ctl.value = vbChecked, True, False))
            cmdTab(1).Enabled = benefit.GetItem(accom_ErOwn)
          End If
        Case 1
          i = (IIf(ctl.value = vbChecked, True, False) <> benefit.GetItem(accom_JobRelated))
          If i <> 0 Then Call benefit.SetItem(accom_JobRelated, IIf(ctl.value = vbChecked, True, False))
        Case 2
          i = (IIf(ctl.value = vbChecked, True, False) <> benefit.GetItem(accom_SixYear))
          If i <> 0 Then Call benefit.SetItem(accom_SixYear, IIf(ctl.value = vbChecked, True, False))
        Case 3
        Case 4
          i = (IIf(ctl.value = vbChecked, True, False) <> benefit.GetItem(accom_isrent))
          If i <> 0 Then
            Call benefit.SetItem(accom_isrent, IIf(ctl.value = vbChecked, True, False))
            If benefit.GetItem(accom_isrent) Then
              TxtBx(1).Enabled = False
              TxtBx(4).Enabled = False
            Else
              TxtBx(1).Enabled = True
              TxtBx(4).Enabled = True
            End If
          End If
        Case 5
        Case Else
          ECASE "Unknown control"
      End Select
    Case Else
      ECASE "Unknown control"
  End Select
  If i <> 0 Then benefit.InvalidFields = InvalidFields(Me)
  If benefit.InvalidFields > 0 Then
    Call MDIMain.sts.SetStatus(0, "", S_NOSAVE)
    Call MDIMain.SetUndo
    benefit.Dirty = False
    Set lst = lb.SelectedItem
    With lst
      .Text = benefit.name
      .SubItems(1) = formatworkingnumber(benefit.Calculate, "")
    End With
  ElseIf i <> 0 Then
    Call MDIMain.sts.SetStatus(0, "", "")
    Call MDIMain.SetConfirmUndo
    benefit.Dirty = True
    Set lst = lb.SelectedItem
    With lst
      .Text = benefit.name
      .SubItems(1) = formatworkingnumber(benefit.Calculate, "")
    End With
  ElseIf benefit.Dirty Then
    Call MDIMain.sts.SetStatus(0, "", "")
    Call MDIMain.SetConfirmUndo
  End If
  
CheckChanged_End:
  Call xReturn("CheckChanged")
  Exit Function

CheckChanged_Err:
  Call ErrorMessage(ERR_ERROR, Err, "CheckChanged", "ERR_CHECKCHANGED", "This function has failed for the form " & Me.name & ".")
  Resume CheckChanged_End
End Function

Private Sub LB_Click()
  If Not (lb.SelectedItem Is Nothing) Then
    lb.SelectedItem.Selected = True
  End If
End Sub

Private Sub LB_ItemClick(ByVal Item As ComctlLib.ListItem)
  If Not (lb.SelectedItem Is Nothing) Then
    Call AccomDetails(Item.Tag)
  End If
End Sub

Private Sub LB_ColumnClick(ByVal ColumnHeader As ComctlLib.ColumnHeader)
  lb.SortKey = ColumnHeader.Index - 1
  lb.SelectedItem.EnsureVisible
End Sub

Private Sub TxtBx_LostFocus(Index As Integer)
  Call CheckChanged(TxtBx(Index))
End Sub
